        <activity
            android:name="com.herokuapps.gymticks_android.RoutePage"
            android:label="@string/app_name" >
        </activity>     
        <activity
            android:name="com.herokuapps.gymticks_android.UserPage"
            android:label="@string/app_name" >
        </activity>     
        </application>

</manifest>